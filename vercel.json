{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/test",
      "destination": "/api/test"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ]
}